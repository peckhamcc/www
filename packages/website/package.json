    "s3-deploy": "^1.2.1",
    "sharp": "^0.25.2",
    "standard": "^16.0.3",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "^4.29.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-livereload-plugin": "^2.2.0"

    "react": "^16.4.0",
    "react-dom": "^16.2.0",
    "react-icons": "^3.3.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-localstorage": "^0.4.1",
    "nanoid": "^3.1.18",
    "styled-components": "^4.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {

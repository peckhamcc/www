{
  "name": "@peckhamcc/website",
  "version": "1.0.0",
  "description": "The Peckham Cycle Club website",
  "homepage": "https://peckhamcc.github.com/www",
  "author": "Alex Potsides",
  "repository": {
    "type": "git",
